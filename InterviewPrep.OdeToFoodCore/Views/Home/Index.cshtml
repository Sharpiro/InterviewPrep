@model InterviewPrep.OdeToFoodCore.ViewModels.HomePageViewModel
@{
    ViewBag.Title = "Home";
}

<table>
    @foreach (var restaurant in Model.Restaurants)
            {
        <tr>
            <td>@Html.ActionLink(restaurant.Id.ToString(), "Details", new { id = restaurant.Id })</td>
            <td>@restaurant.Name</td>
        </tr>
    }
</table>
@Html.ActionLink("Create New", "Create")

@section footer{
    <div>@Model.CurrentGreeting</div>
}