{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Script Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "C:\\Program Files\\dotnet\\dotnet.exe", // path to your dotnet.exe installation
            "args": ["exec", "--additionalprobingpath", "C:\\Users\\sharpiro\\.nuget\\packages", "--depsfile", "C:\\Users\\sharpiro\\.nuget\\packages\\.tools\\Dotnet.Script\\0.9.0-beta\\netcoreapp1.0\\Dotnet.Script.deps.json", "C:\\Users\\sharpiro\\.nuget\\packages\\Dotnet.Script\\0.9.0-beta\\lib\\netcoreapp1.0\\dotnet-script.dll", "${workspaceRoot}\\main.csx","-d"],
            "cwd": "${workspaceRoot}",
            "externalConsole": false,
            "stopAtEntry": true,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command.pickProcess}"
        }
    ]
}